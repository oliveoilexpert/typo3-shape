<T3DataStructure>
    <sheets>
        <general>
            <ROOT>
                <sheetTitle>General</sheetTitle>
                <type>array</type>
                <el>
                    <storagePage>
                        <label>Storage page</label>
                        <config>
                            <type>group</type>
                            <allowed>pages</allowed>
                            <size>1</size>
                            <maxitems>1</maxitems>
                        </config>
                    </storagePage>
                    <excludedFields>
                        <label>Exclude fields from saved data</label>
                        <config>
                            <type>input</type>
                        </config>
                    </excludedFields>
                    <saveUserData>
                        <label>Save User Agent and IP-Address</label>
                        <config>
                            <type>check</type>
                            <renderType>checkboxToggle</renderType>
                        </config>
                    </saveUserData>
                </el>
            </ROOT>
        </general>
    </sheets>
</T3DataStructure>