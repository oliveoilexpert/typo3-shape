<T3DataStructure>
    <sheets>
        <general>
            <ROOT>
                <sheetTitle>General</sheetTitle>
                <type>array</type>
                <el>
                    <storagePage>
                        <label>Storage page</label>
                        <config>
                            <type>group</type>
                            <allowed>pages</allowed>
                            <size>1</size>
                            <maxitems>1</maxitems>
                        </config>
                    </storagePage>
                    <excludedFields>
                        <label>Exclude fields from saved data</label>
                        <config>
                            <type>input</type>
                        </config>
                    </excludedFields>
                    <saveUserData>
                        <label>Save User Agent and IP-Address</label>
                        <config>
                            <type>check</type>
                        </config>
                    </saveUserData>
                    <connectToLanguageParentForm>
                        <label>Connect to original language form</label>
                        <description>Submissions are connected to the form and plugin they were submitted from. When enabled, this will connect them to the original language version of form and plugin, if possible.</description>
                        <config>
                            <type>check</type>
                        </config>
                    </connectToLanguageParentForm>
                </el>
            </ROOT>
        </general>
    </sheets>
</T3DataStructure>