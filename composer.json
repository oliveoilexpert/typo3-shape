{
	"name": "oliveoilexpert/typo3-shape",
	"type": "typo3-cms-extension",
	"description": "Flexible form extension",
	"license": "GPL-2.0-or-later",
	"require": {
		"php": "^8.2",
		"typo3/cms-core": "^13.4",
		"typo3/cms-extbase": "^13.4",
		"typo3/cms-fluid": "^13.4",
		"typo3/cms-frontend": "^13.4",
		"typo3/cms-form": "^13.4"
	},
	"replace": {
		"typo3-ter/shape": "self.version"
	},
	"autoload": {
		"psr-4": {
			"UBOS\\Shape\\": "Classes/"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "shape"
		}
	}
}
